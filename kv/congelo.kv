#:import utils kivy.utils
#:import Gradient kivy_gradient.Gradient
#:import C kivy.utils.get_color_from_hex

<Congelo>:
    FloatLayout:

        canvas:
            Color:
                rgb: utils.get_color_from_hex("#FFFFFF")
            Rectangle:
                size: self.size
                pos: self.pos

        FloatLayout:
            pos_hint: {"top": 1, "left": 1}
            size_hint: 1, 0.15

            ImageButton:
                id: back
                source: "Images/CookStock/Back.png"
                size_hint: 0.5, 0.5
                pos_hint: {'center_x': 0.05, 'center_y': 0.5}
                on_press:
                    app.state = "accueil"
                    app.classify_recettes()
                    app.update_recettes_accueil()
                    app.change_screen("accueil_screen")

            ImageButton:
                id: plus
                source: "Images/CookStock/Plus.png"
                size_hint: 0.2, 0.2
                pos_hint: {'center_x': 0.95, 'center_y': 0.5}
                on_press:
                    app.change_screen("catalogue_a_screen")

            Label:
                id: placard_title
                text: "Votre cong√©lateur"
                font_size: '50dp'
                color: utils.get_color_from_hex("#000000")
                size_hint: 0.2, 0.2
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}

        FloatLayout:
            canvas:
                Color:
                    rgb: C("#78f4b5")
                Rectangle:
                    size: self.size
                    pos: self.pos
            rows: 1
            pos_hint: {"top": 0.85, "left": 1}
            size_hint: 1, 1

            Button:
                id: p1
                background_color: (0,0,0,0)
                color: utils.get_color_from_hex("#E2693E")
                size_hint: 0.08, 0.12
                pos_hint: {'center_x': 0.111, 'center_y': 0.85}
                canvas.before:
                    Color:
                        rgb: 1,1,1
                    RoundedRectangle:
                        size: self.size
                        pos: self.pos
                        radius: [10,10,10,10]
                on_press:
                    app.supp_aliment(0)

            Image:
